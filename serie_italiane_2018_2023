<script id="ad_feedback_handler">
(function(window, document) {
let slotName = 'inlinebottom';
let linkEl = document.querySelector('#' + slotName + '_sponsored');
if (linkEl) {
linkEl.addEventListener('click', function () {
if(window.innerWidth <= 1023){
// <= medium breakpoints
window.location.href = 'https://www.imdb.com/privacy/adpreferences/';
} else {
// > medium breakpoints
//feedback DB schema has 255 chars as the length limit
let pageUrlInBase64 = btoa(window.top.location.href.substr(0, 255))
let adElementId = '#' + slotName;
let adFrame = document.querySelector(adElementId);
let ad = {};
if (adFrame) { ad = adFrame.contentDocument.ad; }
const event = {
context: 'ad-feedback',
isProd: true,
placementMetaData: {
pageUrl: pageUrlInBase64,
pageType: 'imdb_main',
slotName: slotName
},
creativeMetaData: {
adElementId: slotName,
adId: ad.aid || '',
adProgramId: '1015',
adNetworkId: 'CS',
creativeId: ad.cid || ''
},
feedbackOriginContext: {
marketplaceId: 'A1EVAM02EL8SFB'
},
};
window.postMessage(event);
}
});
}
})(window, document);
